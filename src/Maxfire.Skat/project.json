{
  "version": "0.3.0-*",
  "buildOptions": {
    "debugType": "portable",
    "warningsAsErrors": true,
    "compile": {
      "includeFiles": [
        "../CommonAssemblyInfo.cs"
      ]
    }
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [
          "DEBUG"
        ],
        "optimize": false
      }
    },
    "Release": {
      "buildOptions": {
        "define": [
          "RELEASE"
        ],
        "optimize": true
      }
    }
  },
  "dependencies": {
    "NETStandard.Library": "1.6.0"
  },
  "frameworks": {
    "netstandard1.5": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.1"
      }
    },
    "net452": {}
  }
}
